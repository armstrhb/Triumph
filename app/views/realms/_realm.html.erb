<tr data-link="<%= realm_path(realm) %>">
  <td><%= realm.name %></td>
  <td class='text-center'><%= realm.users.length %></td>
  <td class='text-center'><%= realm.achievements.length %></td>
  <td class='text-center'><%= realm.total_points_earned %></td>
  <% if realm.group.nil? %>
    <td class='text-muted'>none</td>
  <% else %>
    <td><%= realm.group.name %></td>
  <% end %>
</tr>
